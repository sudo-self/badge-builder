#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/midnight/Desktop/BADGE/badge-builder/node_modules/.pnpm/vite@7.1.7_@types+node@24.6.1_jiti@2.6.1_lightningcss@1.30.1_terser@5.44.0_yaml@2.8.1/node_modules/vite/bin/node_modules:/Users/midnight/Desktop/BADGE/badge-builder/node_modules/.pnpm/vite@7.1.7_@types+node@24.6.1_jiti@2.6.1_lightningcss@1.30.1_terser@5.44.0_yaml@2.8.1/node_modules/vite/node_modules:/Users/midnight/Desktop/BADGE/badge-builder/node_modules/.pnpm/vite@7.1.7_@types+node@24.6.1_jiti@2.6.1_lightningcss@1.30.1_terser@5.44.0_yaml@2.8.1/node_modules:/Users/midnight/Desktop/BADGE/badge-builder/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/midnight/Desktop/BADGE/badge-builder/node_modules/.pnpm/vite@7.1.7_@types+node@24.6.1_jiti@2.6.1_lightningcss@1.30.1_terser@5.44.0_yaml@2.8.1/node_modules/vite/bin/node_modules:/Users/midnight/Desktop/BADGE/badge-builder/node_modules/.pnpm/vite@7.1.7_@types+node@24.6.1_jiti@2.6.1_lightningcss@1.30.1_terser@5.44.0_yaml@2.8.1/node_modules/vite/node_modules:/Users/midnight/Desktop/BADGE/badge-builder/node_modules/.pnpm/vite@7.1.7_@types+node@24.6.1_jiti@2.6.1_lightningcss@1.30.1_terser@5.44.0_yaml@2.8.1/node_modules:/Users/midnight/Desktop/BADGE/badge-builder/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
